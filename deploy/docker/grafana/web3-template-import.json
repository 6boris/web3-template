          "rawSql": "SELECT\n    info.unique_id,\n    info.chain_id,\n    info.logo_url,\n    info.chain_name,\n    info.chain_env,\n    metrics.latest_block_number,\n    metrics.gas_price / 1e9 AS gas_price,\n    metrics.tx_tps AS tx_tps,\n    metrics.tx_tpd AS tx_tpd,\n    metrics.tx_pending AS tx_pending,\n    metrics.token_price_usd AS token_price_usd,\n    info.explorer_url\nFROM web3.chain_metrics AS metrics\nLEFT JOIN web3.chain_info AS info ON  metrics.chain_unique_id = info.unique_id\nORDER BY info.order_seq DESC\n;",
          "refId": "A",
          "sql": {
            "columns": [
              {
                "parameters": [],
                "type": "function"
              }
            ],
            "groupBy": [
